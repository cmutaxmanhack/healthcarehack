import pandas as pd

# Read the TSV files into DataFrames
DataDf = pd.read_csv("response_to_antidepressant.tsv", sep="\t")
TestDf = pd.read_csv("TestData.tsv", sep="\t")

# Initialize dictionaries and arrays
traitDict = {}
alleleArray = []

# Populate alleleArray with rows from TestDf
for index, row in TestDf.iterrows():
    alleleArray.append(row.tolist())  # Assuming each row contains allele data in a list format

# Iterate over rows in DataDf
for index, row in DataDf.iterrows():
    traitArray = []  # Reset the traitArray for each row
    for allele_list in alleleArray:  # Iterate over the lists in alleleArray
        for allele in allele_list:  # Iterate over alleles in the list
            if allele in row['riskAllele']:  # Assuming row['riskAllele'] is a string or a list
                # Populate traitArray for the current allele
                traitArray.append(row['pValue'])
                traitArray.append(row['pValueAnnotation'])
                traitArray.append(row['traitName'])
                traitArray.append(row['efoTraits'])
                traitArray.append(row['bgTraits'])
                traitDict[allele] = traitArray  # Assign traitArray to the allele in traitDict

# Print the result
print(traitDict)
