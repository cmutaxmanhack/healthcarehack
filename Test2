import numpy
import pandas as pd

DataDf = pd.read_csv("response_to_antidepressant.tsv", sep="\t")
TestDf = pd.read_csv("TestData.tsv", sep="\t")

traitDict = {}
traitArray = []
alleleDict = {}
alleleArray = []

for index, row in TestDf.iterrows():
    alleleArray.append(row.tolist())

print(alleleArray)

for index, column in DataDf.iterrows():
    for allele_list in alleleArray:
        for allele in allele_list:
            if allele in column['riskAllele']:
                traitArray.append(column['pValue'])
                traitArray.append(column['pValueAnnotation'])
                traitArray.append(column['traitName'])
                traitArray.append(column['efoTraits'])
                traitArray.append(column['bgTraits'])
                traitDict[column['riskAllele']] = traitArray
                
traitDf = pd.DataFrame.from_dict(traitDict.items())

print(traitDf)
